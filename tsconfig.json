{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl":".",
    "paths":{
      "@public/*": ["public/*"],

      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
    
      "@styles/*": ["src/styles/*"],
      "@libs/*": ["src/libs/*"],
      "@scripts/*": ["src/scripts/*"],
    }
  }
}