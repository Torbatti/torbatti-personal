{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl":".",
    "paths":{
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
    
      "@styles/*": ["src/styles/*"],
      "@libs/*": ["src/libs/*"],
    
      "@public/*": ["public/*"],

      "@ui/*": ["../_ui/*"],
      "@ui/astro/*": ["../_ui/astro/*"]
    }
  }
}